<h1>投稿一覧</h1>
  <div class="row">
    <div class="col-3">
      <div>
        <h5>カテゴリ</h5>
      </div>
      <div>
        <h5>地域</h5>
      </div>
    </div>
    <div class="col-9">
        <!--検索結果-->
      <% @posts.each do |post| %>
        <div>
          <p><%= post.name %></p>
          <p><%= post.address %></p>
          <p><%= image_tag post.get_image(100,100) %></p>
        </div>
      <% end %>
    </div>
  </div>
